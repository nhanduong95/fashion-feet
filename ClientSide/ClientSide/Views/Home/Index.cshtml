@model IEnumerable<ClientSide.Models.Category>
@{
    ViewBag.Title = "Home Page";
}
<header>
    <div class="header-container">
        <div class="row">
            <div class="col-lg-12">
                <div class="intro-text">
                    <span class="name">FASHION FEET</span>
                    <hr class="star-light">
                    <span class="slogan">YOUR FEET - YOUR STYLE</span>
                </div>
            </div>
        </div>
    </div>
</header>

<section id="product">
    <div class="container">
        <div class="row">
            @foreach (var cate in Model)
            {
                <div class="col-md-4 cate-home">
                    <a href="@Url.Action("Index", "Products", new { cateId = cate.CateId, cateName = cate.CateName })">
                        <h2>@Html.DisplayFor(modelItem => cate.CateName)</h2>
                    </a>
                       
                    <img src="@Html.DisplayFor(modelItem => cate.CateImageUrl)"
                         alt="@Html.DisplayFor(modelItem => cate.CateName)" class="img-circle" height="280" width="280"/>
                    <p>@Html.DisplayFor(modelItem => cate.CateDes)</p>
                    <a href="@Url.Action("Index", "Products", new { cateId = cate.CateId, cateName = cate.CateName })" class="btn btn-custom-01"
                       style="position: relative; left: 15px; top: -3px;">
                        <i class="fa fa-plus-circle"></i> See More
                    </a>
                </div>
            }
        </div>
    </div>
</section>